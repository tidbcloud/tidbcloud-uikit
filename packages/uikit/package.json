{
  "name": "@tidbcloud/uikit",
  "version": "0.4.2",
  "description": "tidbcloud uikit",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "test",
    "copy:raw-icon": "mkdir -p ./dist/icons/raw && cp -r ./src/icons/raw/ ./dist/icons/raw/",
    "gen:icon": "tsx ./scripts/icon.ts",
    "build": "pnpm gen:icon && tsup"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:tidbcloud/tidbcloud-uikit.git"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "5.10.4",
    "@mantine/core": "5.10.4",
    "@mantine/dates": "5.10.4",
    "@mantine/dropzone": "5.10.4",
    "@mantine/hooks": "5.10.4",
    "@mantine/modals": "5.10.4",
    "@mantine/notifications": "5.10.4",
    "@mantine/prism": "5.10.4",
    "@mantine/styles": "5.10.4",
    "@mantine/utils": "5.10.4",
    "ahooks": "^3.7.7",
    "dayjs": "^1.11.5",
    "dompurify": "^3.0.3",
    "embla-carousel-react": "^7.0.5",
    "google-libphonenumber": "^3.2.30",
    "lodash-es": "^4.17.21",
    "mantine-react-table": "0.9.5",
    "react-table": "^7.8.0",
    "react-table-sticky": "^1.1.3",
    "react-window": "^1.8.9"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./carousel": {
      "require": "./dist/carousel.cjs",
      "import": "./dist/carousel.js"
    },
    "./dates": {
      "require": "./dist/dates.cjs",
      "import": "./dist/dates.js"
    },
    "./hooks": {
      "require": "./dist/hooks.cjs",
      "import": "./dist/hooks.js"
    },
    "./icons": {
      "require": "./dist/icons.cjs",
      "import": "./dist/icons.js"
    },
    "./icons/raw/*": "./dist/icons/raw/*.svg",
    "./modals": {
      "require": "./dist/modals.cjs",
      "import": "./dist/modals.js"
    },
    "./notifications": {
      "require": "./dist/notifications.cjs",
      "import": "./dist/notifications.js"
    },
    "./prism": {
      "require": "./dist/prism.cjs",
      "import": "./dist/prism.js"
    },
    "./theme": {
      "require": "./dist/theme.cjs",
      "import": "./dist/theme.js"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.js"
    },
    "./dropzone": {
      "require": "./dist/dropzone.cjs",
      "import": "./dist/dropzone.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.4",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@types/react-table": "^7.7.12",
    "@types/react-window": "^1.8.5",
    "markdown-table": "^3.0.3",
    "react-phone-input-2": "^2.15.1",
    "tsx": "^3.12.7",
    "typescript": "~4.7.4"
  }
}
