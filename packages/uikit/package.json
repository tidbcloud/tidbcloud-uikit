{
  "name": "@tidbcloud/uikit",
  "version": "0.0.6",
  "description": "tidbcloud uikit",
  "main": "dist/index.js",
  "module": "dis/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "test",
    "gen:icon": "tsx ./scripts/icon.ts",
    "build": "pnpm gen:icon && tsup"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:tidbcloud/tidbcloud-uikit.git"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/icons/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "bundle": false
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "5.10.4",
    "@mantine/core": "5.10.4",
    "@mantine/dates": "5.10.4",
    "@mantine/dropzone": "5.10.4",
    "@mantine/form": "5.10.4",
    "@mantine/hooks": "5.10.4",
    "@mantine/modals": "5.10.4",
    "@mantine/notifications": "5.10.4",
    "@mantine/prism": "5.10.4",
    "@mantine/styles": "5.10.4",
    "@mantine/utils": "5.10.4",
    "ahooks": "^3.7.7",
    "dayjs": "^1.11.5",
    "dompurify": "^3.0.3",
    "embla-carousel-react": "^7.0.5",
    "google-libphonenumber": "^3.2.30",
    "lodash-es": "^4.17.21",
    "mantine-react-table": "0.9.5",
    "react-hook-form": "6.15.8",
    "react-phone-input-2": "^2.15.1",
    "react-table": "^7.8.0",
    "react-table-sticky": "^1.1.3",
    "react-window": "^1.8.9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "require": "./dist/icons/index.js",
      "import": "./dist/icons/index.mjs"
    },
    "./form": {
      "types": "./dist/form/index.d.ts",
      "require": "./dist/form/index.js",
      "import": "./dist/form/index.mjs"
    }
  }
}
