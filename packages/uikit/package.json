{
  "name": "@tidbcloud/uikit",
  "version": "0.0.24",
  "description": "tidbcloud uikit",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "test",
    "copy:raw-icon": "mkdir -p ./dist/icons/raw && cp -r ./src/icons/raw/ ./dist/icons/raw/",
    "gen:icon": "tsx ./scripts/icon.ts",
    "build": "pnpm gen:icon && tsup && pnpm run copy:raw-icon"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:tidbcloud/tidbcloud-uikit.git"
  },
  "tsup": {
    "entry": {
      "carousel": "src/carousel/index.ts",
      "index": "src/components/index.ts",
      "dates": "src/dates/index.ts",
      "form": "src/form/index.ts",
      "hooks": "src/hooks/index.ts",
      "icons": "src/icons/index.ts",
      "modals": "src/modals/index.ts",
      "notifications": "src/notifications/index.ts",
      "prism": "src/prism/index.ts",
      "theme": "src/theme/index.ts",
      "utils": "src/utils/index.ts"
    },
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "bundle": true,
    "external": [
      "react",
      "react-dom"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "5.10.4",
    "@mantine/core": "5.10.4",
    "@mantine/dates": "5.10.4",
    "@mantine/dropzone": "5.10.4",
    "@mantine/form": "5.10.4",
    "@mantine/hooks": "5.10.4",
    "@mantine/modals": "5.10.4",
    "@mantine/notifications": "5.10.4",
    "@mantine/prism": "5.10.4",
    "@mantine/styles": "5.10.4",
    "@mantine/utils": "5.10.4",
    "ahooks": "^3.7.7",
    "dayjs": "^1.11.5",
    "dompurify": "^3.0.3",
    "embla-carousel-react": "^7.0.5",
    "google-libphonenumber": "^3.2.30",
    "lodash-es": "^4.17.21",
    "mantine-react-table": "0.9.5",
    "react-hook-form": "6.15.8",
    "react-phone-input-2": "^2.15.1",
    "react-table": "^7.8.0",
    "react-table-sticky": "^1.1.3",
    "react-window": "^1.8.9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./carousel": {
      "types": "./dist/carousel.d.ts",
      "require": "./dist/carousel.js",
      "import": "./dist/carousel.mjs"
    },
    "./dates": {
      "types": "./dist/dates.d.ts",
      "require": "./dist/dates.js",
      "import": "./dist/dates.mjs"
    },
    "./form": {
      "types": "./dist/form.d.ts",
      "require": "./dist/form.js",
      "import": "./dist/form.mjs"
    },
    "./hooks": {
      "types": "./dist/hoos.d.ts",
      "require": "./dist/hooks.js",
      "import": "./dist/hooks.mjs"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "require": "./dist/icons.js",
      "import": "./dist/icons.mjs"
    },
    "./modals": {
      "types": "./dist/modals.d.ts",
      "require": "./dist/modals.js",
      "import": "./dist/modals.mjs"
    },
    "./notifications": {
      "types": "./dist/notifications.d.ts",
      "require": "./dist/notifications.js",
      "import": "./dist/notifications.mjs"
    },
    "./prism": {
      "types": "./dist/prism.d.ts",
      "require": "./dist/prism.js",
      "import": "./dist/prism.mjs"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "require": "./dist/theme.js",
      "import": "./dist/theme.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  }
}
