{
  "extends": "../../tsconfig.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_app.js",
    "components/Dynamics.js",
    "pages/sql-editor.mdx",
    "pages/sql-editor-doc.mdx",
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "incremental": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@lib/*": ["lib/*"]
    }
  }
}
