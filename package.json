    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "markdown-table": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
